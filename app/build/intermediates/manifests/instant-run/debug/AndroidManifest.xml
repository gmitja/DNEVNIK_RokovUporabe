<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mitjag.dnevnik_rokov"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        name="com.example.mitjag.dnevnik_rokov.ApplicationMy"
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.mitjag.dnevnik_rokov.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mitjag.dnevnik_rokov.AddActivity" />

        <receiver android:name="com.example.mitjag.dnevnik_rokov.Notification_receiver" >
            <action android:name="android.intent.action.MAIN" />
        </receiver>
    </application>

</manifest>